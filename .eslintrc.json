{
    "env": {
        "es2021": true,
        "node": true
    },
    "extends": [
        "xo"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "no-return-assign":"off"
    }
}
